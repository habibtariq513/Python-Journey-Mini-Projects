   "source": [
    "import random\n",
    "import string\n",
    "\n",
    "pass_len = 12\n",
    "char_var = string.digits + string.ascii_letters + string.punctuation\n",
    "\n",
    "password = \"\"\n",
    "for i in range(pass_len):\n",
    "    password += random.choice(char_var)\n",
    "print(password)\n"
   ]
